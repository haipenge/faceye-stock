<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<!-- 模块:stock 业务对像:FinancialData 管理后台 tiles模板配置 -->
	<!-- 作者:haipege -->
	<!-- FinancialData 管理 首页(列表页) -->
	<definition name="stock.financialData.manager" extends="default.manager">
		<put-attribute name="default-center" value="stock.financialData.center"></put-attribute>
	</definition>
	<definition name="stock.financialData.center" extends="default.center">
		<put-attribute name="default-center-right" value="/component/stock/financialData/financialData_center.jsp"></put-attribute>
	</definition>

	<!-- FinancialData 新增,编辑页面 -->
	<definition name="stock.financialData.update" extends="default.manager">
		<put-attribute name="default-center" value="stock.financialData.update.center"></put-attribute>
	</definition>
	<definition name="stock.financialData.update.center" extends="default.center">
		<put-attribute name="default-center-right" value="/component/stock/financialData/financialData_update.jsp"></put-attribute>
	</definition>

	<!-- FinancialData 明细页 -->
	<definition name="stock.financialData.detail" extends="default.manager">
		<put-attribute name="default-center" value="stock.financialData.detail.center"></put-attribute>
	</definition>
	<definition name="stock.financialData.detail.center" extends="default.center">
		<put-attribute name="default-center-right" value="/component/stock/financialData/financialData_detail.jsp"></put-attribute>
	</definition>

	<!-- FinancialData Report 数据报告（利润表等） -->
	<definition name="stock.financialData.report" extends="default.manager">
		<put-attribute name="default-center" value="stock.financialData.report.center"></put-attribute>
	</definition>
	<definition name="stock.financialData.report.center" extends="default.center">
		<put-attribute name="default-center-right" value="/component/stock/financialData/financialData_report.jsp"></put-attribute>
	</definition>
	<definition name="stock.financialData.wrapReporter" extends="default.manager">
		<put-attribute name="default-center" value="stock.financialData.wrapReporter.center"></put-attribute>
	</definition>
	<definition name="stock.financialData.wrapReporter.center" extends="default.center">
		<put-attribute name="default-center-right" value="/component/stock/financialData/wrapReporter_center.jsp"></put-attribute>
	</definition>
</tiles-definitions>