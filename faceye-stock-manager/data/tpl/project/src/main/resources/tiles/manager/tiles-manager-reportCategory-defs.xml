    <!-- ReportCategory 管理 首页(列表页) -->
	<definition name="stock.reportCategory.manager" extends="default.manager">
	    <put-attribute name="default-center" value="stock.reportCategory.center"></put-attribute>
	</definition>
	<definition name="stock.reportCategory.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/reportCategory/reportCategory_center.jsp"></put-attribute>
