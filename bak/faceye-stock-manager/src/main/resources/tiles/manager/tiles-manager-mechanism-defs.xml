<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:Mechanism 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Mechanism 管理 首页(列表页) -->
	<definition name="stock.mechanism.manager" extends="default.manager">
	    <put-attribute name="default-center" value="stock.mechanism.center"></put-attribute>
	</definition>
	<definition name="stock.mechanism.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/mechanism/mechanism_center.jsp"></put-attribute>
	</definition>
	
	<!-- Mechanism 新增,编辑页面 -->
	<definition name="stock.mechanism.update" extends="default.manager">
	    <put-attribute name="default-center" value="stock.mechanism.update.center"></put-attribute>
	</definition>
	<definition name="stock.mechanism.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/mechanism/mechanism_update.jsp"></put-attribute>
	</definition>
	
	<!-- Mechanism 明细页 -->
	<definition name="stock.mechanism.detail" extends="default.manager">
	    <put-attribute name="default-center" value="stock.mechanism.detail.center"></put-attribute>
	</definition>
	<definition name="stock.mechanism.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/mechanism/mechanism_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>