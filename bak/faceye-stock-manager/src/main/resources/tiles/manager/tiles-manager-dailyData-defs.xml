<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:DailyData 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- DailyData 管理 首页(列表页) -->
	<definition name="stock.dailyData.manager" extends="default.manager">
	    <put-attribute name="default-center" value="stock.dailyData.center"></put-attribute>
	</definition>
	<definition name="stock.dailyData.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/dailyData/dailyData_center.jsp"></put-attribute>
	</definition>
	
	<!-- DailyData 新增,编辑页面 -->
	<definition name="stock.dailyData.update" extends="default.manager">
	    <put-attribute name="default-center" value="stock.dailyData.update.center"></put-attribute>
	</definition>
	<definition name="stock.dailyData.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/dailyData/dailyData_update.jsp"></put-attribute>
	</definition>
	
	<!-- DailyData 明细页 -->
	<definition name="stock.dailyData.detail" extends="default.manager">
	    <put-attribute name="default-center" value="stock.dailyData.detail.center"></put-attribute>
	</definition>
	<definition name="stock.dailyData.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/dailyData/dailyData_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>