<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:TotalStock 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- TotalStock 管理 首页(列表页) -->
	<definition name="stock.totalStock.manager" extends="default.manager">
	    <put-attribute name="default-center" value="stock.totalStock.center"></put-attribute>
	</definition>
	<definition name="stock.totalStock.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/totalStock/totalStock_center.jsp"></put-attribute>
	</definition>
	
	<!-- TotalStock 新增,编辑页面 -->
	<definition name="stock.totalStock.update" extends="default.manager">
	    <put-attribute name="default-center" value="stock.totalStock.update.center"></put-attribute>
	</definition>
	<definition name="stock.totalStock.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/totalStock/totalStock_update.jsp"></put-attribute>
	</definition>
	
	<!-- TotalStock 明细页 -->
	<definition name="stock.totalStock.detail" extends="default.manager">
	    <put-attribute name="default-center" value="stock.totalStock.detail.center"></put-attribute>
	</definition>
	<definition name="stock.totalStock.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/totalStock/totalStock_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>