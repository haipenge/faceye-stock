<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:DailyStat 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- DailyStat 管理 首页(列表页) -->
	<definition name="stock.dailyStat.manager" extends="default.manager">
	    <put-attribute name="default-center" value="stock.dailyStat.center"></put-attribute>
	</definition>
	<definition name="stock.dailyStat.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/dailyStat/dailyStat_center.jsp"></put-attribute>
	</definition>
	
	<!-- DailyStat 新增,编辑页面 -->
	<definition name="stock.dailyStat.update" extends="default.manager">
	    <put-attribute name="default-center" value="stock.dailyStat.update.center"></put-attribute>
	</definition>
	<definition name="stock.dailyStat.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/dailyStat/dailyStat_update.jsp"></put-attribute>
	</definition>
	
	<!-- DailyStat 明细页 -->
	<definition name="stock.dailyStat.detail" extends="default.manager">
	    <put-attribute name="default-center" value="stock.dailyStat.detail.center"></put-attribute>
	</definition>
	<definition name="stock.dailyStat.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/dailyStat/dailyStat_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>