<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:AccountingElement 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- AccountingElement 管理 首页(列表页) -->
	<definition name="stock.accountingElement.manager" extends="default.manager">
	    <put-attribute name="default-center" value="stock.accountingElement.center"></put-attribute>
	</definition>
	<definition name="stock.accountingElement.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/accountingElement/accountingElement_center.jsp"></put-attribute>
	</definition>
	
	<!-- AccountingElement 新增,编辑页面 -->
	<definition name="stock.accountingElement.update" extends="default.manager">
	    <put-attribute name="default-center" value="stock.accountingElement.update.center"></put-attribute>
	</definition>
	<definition name="stock.accountingElement.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/accountingElement/accountingElement_update.jsp"></put-attribute>
	</definition>
	
	<!-- AccountingElement 明细页 -->
	<definition name="stock.accountingElement.detail" extends="default.manager">
	    <put-attribute name="default-center" value="stock.accountingElement.detail.center"></put-attribute>
	</definition>
	<definition name="stock.accountingElement.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/stock/accountingElement/accountingElement_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>