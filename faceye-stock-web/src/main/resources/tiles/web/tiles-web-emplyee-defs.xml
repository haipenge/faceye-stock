<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:crm 业务对像:Emplyee 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Emplyee 管理 首页(列表页) -->
	<definition name="crm.emplyee.web.home" extends="default.web">
	    <put-attribute name="default-center" value="crm.emplyee.center"></put-attribute>
	</definition>
	<definition name="crm.emplyee.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/crm/emplyee/emplyee_center.jsp"></put-attribute>
	</definition>
	
	<!-- Emplyee 新增,编辑页面 -->
	<definition name="crm.emplyee.web.update" extends="default.web">
	    <put-attribute name="default-center" value="crm.emplyee.update.web.center"></put-attribute>
	</definition>
	<definition name="crm.emplyee.update.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/crm/emplyee/emplyee_update.jsp"></put-attribute>
	</definition>
	
	<!-- Emplyee 明细页 -->
	<definition name="crm.emplyee.web.detail" extends="default.web">
	    <put-attribute name="default-center" value="crm.emplyee.detail.web.center"></put-attribute>
	</definition>
	<definition name="crm.emplyee.detail.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/crm/emplyee/emplyee_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>