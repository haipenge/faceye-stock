<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:FinancialReport 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- FinancialReport 管理 首页(列表页) -->
	<definition name="stock.financialReport.web.home" extends="default.web">
	    <put-attribute name="default-center" value="stock.financialReport.center"></put-attribute>
	</definition>
	<definition name="stock.financialReport.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/stock/financialReport/financialReport_center.jsp"></put-attribute>
	</definition>
	
	<!-- FinancialReport 新增,编辑页面 -->
	<definition name="stock.financialReport.web.update" extends="default.web">
	    <put-attribute name="default-center" value="stock.financialReport.update.web.center"></put-attribute>
	</definition>
	<definition name="stock.financialReport.update.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/stock/financialReport/financialReport_update.jsp"></put-attribute>
	</definition>
	
	<!-- FinancialReport 明细页 -->
	<definition name="stock.financialReport.web.detail" extends="default.web">
	    <put-attribute name="default-center" value="stock.financialReport.detail.web.center"></put-attribute>
	</definition>
	<definition name="stock.financialReport.detail.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/stock/financialReport/financialReport_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>