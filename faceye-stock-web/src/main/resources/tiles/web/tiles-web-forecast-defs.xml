<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:stock 业务对像:Forecast 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Forecast 管理 首页(列表页) -->
	<definition name="stock.forecast.web.home" extends="default.web">
	    <put-attribute name="default-center" value="stock.forecast.center"></put-attribute>
	</definition>
	<definition name="stock.forecast.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/stock/forecast/forecast_center.jsp"></put-attribute>
	</definition>
	
	<!-- Forecast 新增,编辑页面 -->
	<definition name="stock.forecast.web.update" extends="default.web">
	    <put-attribute name="default-center" value="stock.forecast.update.web.center"></put-attribute>
	</definition>
	<definition name="stock.forecast.update.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/stock/forecast/forecast_update.jsp"></put-attribute>
	</definition>
	
	<!-- Forecast 明细页 -->
	<definition name="stock.forecast.web.detail" extends="default.web">
	    <put-attribute name="default-center" value="stock.forecast.detail.web.center"></put-attribute>
	</definition>
	<definition name="stock.forecast.detail.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/stock/forecast/forecast_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>