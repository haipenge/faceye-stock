<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:crm 业务对像:Apple 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Apple 管理 首页(列表页) -->
	<definition name="crm.apple.web.home" extends="default.web">
	    <put-attribute name="default-center" value="crm.apple.center"></put-attribute>
	</definition>
	<definition name="crm.apple.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/crm/apple/apple_center.jsp"></put-attribute>
	</definition>
	
	<!-- Apple 新增,编辑页面 -->
	<definition name="crm.apple.web.update" extends="default.web">
	    <put-attribute name="default-center" value="crm.apple.update.web.center"></put-attribute>
	</definition>
	<definition name="crm.apple.update.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/crm/apple/apple_update.jsp"></put-attribute>
	</definition>
	
	<!-- Apple 明细页 -->
	<definition name="crm.apple.web.detail" extends="default.web">
	    <put-attribute name="default-center" value="crm.apple.detail.web.center"></put-attribute>
	</definition>
	<definition name="crm.apple.detail.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/crm/apple/apple_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>